<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="fr.wcs.wildcommunitysocks.Flux"
    android:background="@color/colorWhite">


    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageView
            android:id="@+id/imageViewAllRanking"
            android:layout_width="0dp"
            android:layout_height="255dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.01"
            app:layout_constraintHorizontal_bias="0.0"
            app:srcCompat="@drawable/trophy"/>

        <ImageView
            android:id="@+id/imageViewCategory1"
            android:layout_width="66dp"
            android:layout_height="78dp"
            android:layout_marginRight="8dp"
            android:layout_marginLeft="31dp"
            android:layout_marginBottom="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/imageViewCategory2"
            app:layout_constraintVertical_bias="0.775"
            app:layout_constraintHorizontal_bias="0.0"
            app:srcCompat="@drawable/chaussette"
            android:layout_marginStart="31dp"
            android:layout_marginEnd="8dp" />

        <ImageView
            android:id="@+id/imageViewCategory3"
            android:layout_width="66dp"
            android:layout_height="69dp"
            android:layout_marginRight="64dp"
            android:layout_marginBottom="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintVertical_bias="0.832"
            app:srcCompat="@drawable/tong"
            android:layout_marginEnd="40dp"/>

        <ImageView
            android:id="@+id/imageViewCategory2"
            android:layout_width="99dp"
            android:layout_height="50dp"
            android:layout_marginRight="24dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/imageViewCategory3"
            app:layout_constraintVertical_bias="0.813"
            app:srcCompat="@drawable/chausson"
            android:layout_marginEnd="24dp" />

    </android.support.constraint.ConstraintLayout>

</FrameLayout>