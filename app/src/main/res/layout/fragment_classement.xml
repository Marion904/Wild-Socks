<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="fr.wcs.wildcommunitysocks.Flux"
    android:background="#FFFFFF">


    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageViewAllRanking"
            android:layout_width="400dp"
            android:layout_height="240dp"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:cropToPadding="false"
            android:src="@mipmap/group"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/constraintLayout"
            android:layout_marginTop="125dp" />

        <ImageView
            android:id="@+id/imageViewCategory1"
            android:layout_width="wrap_content"
            android:layout_height="109dp"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="23dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="23dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:src="@mipmap/chaussette"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.809"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/imageView6"
            app:layout_constraintTop_toBottomOf="@+id/imageViewAllRanking"
            app:layout_constraintVertical_bias="0.264" />

        <ImageView
            android:id="@+id/imageViewCategory2"
            android:layout_width="92dp"
            android:layout_height="53dp"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="55dp"
            android:layout_marginRight="55dp"
            android:layout_marginTop="8dp"
            android:layout_weight="0.33"
            android:src="@mipmap/charentaise"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/imageViewCategory3"
            app:layout_constraintTop_toBottomOf="@+id/imageViewAllRanking"
            app:layout_constraintVertical_bias="0.322" />

        <ImageView
            android:id="@+id/imageViewCategory3"
            android:layout_width="wrap_content"
            android:layout_height="109dp"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="26dp"
            android:layout_marginRight="26dp"
            android:layout_marginTop="8dp"
            android:layout_weight="0.07"
            android:src="@mipmap/havaianas17tong"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageViewAllRanking"
            app:layout_constraintVertical_bias="0.264" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="18dp"
            android:layout_height="232dp"
            android:layout_marginEnd="19dp"
            android:layout_marginRight="19dp"
            app:layout_constraintRight_toLeftOf="@+id/imageViewCategory2"
            app:layout_constraintTop_toBottomOf="@+id/imageViewAllRanking"
            app:srcCompat="@mipmap/line" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="15dp"
            android:layout_height="272dp"
            android:layout_marginLeft="22dp"
            android:layout_marginStart="22dp"
            app:layout_constraintLeft_toRightOf="@+id/imageViewCategory2"
            app:layout_constraintTop_toBottomOf="@+id/imageViewAllRanking"
            app:srcCompat="@mipmap/line" />

        <android.support.constraint.ConstraintLayout
            android:layout_width="375dp"
            android:layout_height="116dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="0dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/imageViewAllRanking"
            app:layout_constraintVertical_bias="0.0"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:id="@+id/constraintLayout"
            app:layout_constraintHorizontal_bias="0.5">

            <TextView
                android:id="@+id/textViewDisplayName"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_marginLeft="27dp"
                android:layout_marginStart="27dp"
                android:layout_marginTop="20dp"
                android:text="TextView"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/profile_image" />

            <de.hdodenhof.circleimageview.CircleImageView
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/profile_image"
                android:layout_width="58dp"
                android:layout_height="57dp"
                app:civ_border_width="2dp"

                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="16dp"
                android:layout_marginLeft="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginStart="27dp" />

            <Button
                android:id="@+id/buttonModifyProfil"
                android:layout_width="147dp"
                android:layout_height="29dp"
                android:background="@drawable/button_modify_profil"
                android:text="Modifier mon profil"
                android:textSize="12sp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="8dp"
                app:layout_constraintLeft_toRightOf="@+id/textViewDisplayName"
                android:layout_marginLeft="83dp"
                android:layout_marginRight="8dp"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintHorizontal_bias="0.192"
                android:layout_marginStart="83dp"
                android:layout_marginEnd="8dp" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Pointure"
                android:textSize="12sp"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="13dp"
                app:layout_constraintRight_toLeftOf="@+id/imageView5"
                android:layout_marginRight="8dp"
                app:layout_constraintLeft_toRightOf="@+id/profile_image"
                android:layout_marginLeft="8dp"
                app:layout_constraintHorizontal_bias="0.833"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="8dp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Publication"
                android:textSize="12sp"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="13dp"
                app:layout_constraintLeft_toRightOf="@+id/imageView5"
                android:layout_marginLeft="26dp"
                app:layout_constraintRight_toLeftOf="@+id/imageView4"
                android:layout_marginRight="8dp"
                app:layout_constraintHorizontal_bias="1.0"
                android:layout_marginStart="26dp"
                android:layout_marginEnd="8dp" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nbr KickUp"
                android:textSize="12sp"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="13dp"
                app:layout_constraintLeft_toRightOf="@+id/imageView4"
                android:layout_marginLeft="12dp"
                android:layout_marginStart="12dp" />

            <TextView
                android:id="@+id/textViewPointure"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:layout_marginLeft="62dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="62dp"
                android:gravity="center"
                android:text="42"
                android:textSize="@dimen/_18sdp"
                app:layout_constraintLeft_toRightOf="@+id/profile_image"
                app:layout_constraintRight_toLeftOf="@+id/imageView5"
                android:layout_marginBottom="15dp"
                app:layout_constraintBottom_toTopOf="@+id/buttonModifyProfil"
                app:layout_constraintHorizontal_bias="0.388" />

            <TextView
                android:id="@+id/textViewPublish"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:gravity="center"
                android:text="42"
                android:textSize="@dimen/_18sdp"
                android:layout_marginBottom="14dp"
                app:layout_constraintBottom_toTopOf="@+id/buttonModifyProfil"
                app:layout_constraintRight_toLeftOf="@+id/imageView4"
                android:layout_marginRight="8dp"
                app:layout_constraintLeft_toRightOf="@+id/imageView5"
                android:layout_marginLeft="8dp"
                app:layout_constraintHorizontal_bias="0.64" />

            <TextView
                android:id="@+id/textViewNbrKickUp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="14dp"
                android:layout_marginLeft="27dp"
                android:layout_marginStart="31dp"
                android:gravity="center"
                android:text="42"
                android:textSize="@dimen/_18sdp"
                app:layout_constraintBottom_toTopOf="@+id/buttonModifyProfil"
                app:layout_constraintLeft_toRightOf="@+id/imageView4" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@mipmap/littleline"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toTopOf="@+id/buttonModifyProfil"
                app:layout_constraintVertical_bias="0.0"
                android:layout_marginRight="82dp"
                app:layout_constraintRight_toRightOf="parent" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@mipmap/littleline"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toTopOf="@+id/buttonModifyProfil"
                app:layout_constraintVertical_bias="0.0"
                app:layout_constraintLeft_toRightOf="@+id/profile_image"
                android:layout_marginLeft="130dp" />
        </android.support.constraint.ConstraintLayout>

    </android.support.constraint.ConstraintLayout>

</FrameLayout>